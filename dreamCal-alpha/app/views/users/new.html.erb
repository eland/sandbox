<h2>Create a new user</h2>

<%= form_for @user do |f| %>
  <%= f.label :name %><br />
  <%= f.text_field :name %><br />
  <%= f.label :goal_title %><br />
  <%= f.text_field :goal_title %><br />
  <%= f.label :goal_description %><br />
  <%= f.text_area :goal_description %><br />
  <%= f.submit %>
<% end %>
